{
  "name": "ngx-monaco-editor-v15",
  "version": "16.2.7",
  "description": "Monaco Code Editor For Angular 15",
  "keywords": [
    "angular",
    "monaco",
    "monaco-editor-v15",
    "code",
    "editor",
    "reusable"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-s build-editor serve",
    "serve": "ng serve",
    "build": "ng build --project=editor-tester --base-href=editor-tester --configuration=development",
    "build:prod": "ng build --project=editor-tester --base-href=editor-tester",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-editor": "npm run copy:readme && ng build editor",
    "npm-publish": "npm run build-editor && bash npm-publish",
    "copy:readme": "cp -rf README.md projects/editor/"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/athreyacodes/ngx-monaco-editor-v15.git"
  },
  "bugs": {
    "url": "https://github.com/athreyacodes/ngx-monaco-editor-v15/issues"
  },
  "author": "Athreya",
  "contributors": [
    "Athreya <athreyamr10@gmail.com>"
  ],
  "dependencies": {
    "@angular/animations": ">=16.2.7",
    "@angular/common": ">=16.2.7",
    "@angular/compiler": ">=16.2.7",
    "@angular/core": ">=16.2.7",
    "@angular/forms": ">=16.2.7",
    "@angular/platform-browser": ">=16.2.7",
    "@angular/platform-browser-dynamic": ">=16.2.7",
    "@angular/router": ">=16.2.7",
    "monaco-editor": ">=0.39.0",
    "rxjs": ">=7.8.1",
    "tslib": ">=2.5.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular/cli": "16.2.4",
    "@angular/compiler-cli": "16.2.7",
    "@angular/language-service": "16.2.7",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.16.18",
    "jasmine-core": "4.6.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "16.2.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.9.5"
  }
}
